{
  "name": "@clibu/loader",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Configuration loader for Clibu (TS/JS/JSON)",
  "homepage": "https://github.com/kazvizian/clibu-js",
  "author": {
    "name": "KazViz",
    "email": "kaz@kazviz.com",
    "url": "https://kazviz.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazvizian/clibu-js.git",
    "directory": "packages/loader"
  },
  "bugs": {
    "url": "https://github.com/kazvizian/clibu-js/issues"
  },
  "scripts": {
    "build": "packlet-build --external-auto",
    "clean": "rm -rf dist .tsbuildinfo",
    "test": "bun test"
  },
  "keywords": [
    "kazviz",
    "kazvizian",
    "clibu",
    "loader",
    "cli"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@clibu/core": "^0.1.0"
  },
  "engines": {
    "bun": ">=1.2.0",
    "node": ">=18"
  },
  "packageManager": "bun@1.3.1",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packlet": {
    "gpr": true,
    "gprName": "clibu-loader"
  },
  "sideEffects": false,
  "peerDependencies": {
    "typescript": "^5.9.3"
  }
}
